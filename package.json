{
  "name": "habit-stitch",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm type-check && pnpm build-only",
    "preview": "pnpm build && vite preview",
    "wrangler:deploy": "pnpm build && wrangler deploy",
    "wrangler:local-db": "wrangler d1 execute habit-stitch-db --local --command=\"SELECT 1\"",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "drizzle:generate": "pnpm drizzle-kit generate",
    "drizzle:push-dev": "pnpm drizzle-kit push",
    "drizzle:studio": "pnpm drizzle-kit studio",
    "drizzle:migrate-prod": "pnpm drizzle-kit migrate"
  },
  "dependencies": {
    "@types/vue-feather-icons": "^5.0.8",
    "drizzle-orm": "^0.44.5",
    "feather-icons": "^4.29.2",
    "hono": "^4.9.5",
    "pocketbase": "^0.26.0",
    "tinygradient": "^2.0.1",
    "vue": "^3.5.13",
    "vue-feather-icons": "^5.1.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.12.1",
    "@libsql/client": "^0.15.14",
    "@tsconfig/node22": "^22.0.1",
    "@types/feather-icons": "^4.29.4",
    "@types/node": "^22.14.0",
    "@vite-pwa/assets-generator": "^1.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
}
